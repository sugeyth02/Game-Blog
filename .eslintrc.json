{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-console": 0
  }
}
